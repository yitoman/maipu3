.no-padding
	padding 0
.no-margin
	margin 0
.no-pad-left
	padding-left 0
.no-pad-right
	padding-right 0
.center-block
	display block!important
	float none
	margin-left auto
	margin-right auto
.relative
	position relative
.titleCase
	text-transform capitalize!important
.upperCase
	text-transform uppercase!important
.bg-white
	background white
.m-tb-15
	margin-top 15%
	margin-bottom 15%
.padding-lr-20
	padding-left 20px
	padding-right 20px
.padding-tb-20
	padding-top 20px
	padding-bottom 20px
.padding-tb-10
	padding-top 10px
	padding-bottom 10px
.border-tb-gris
	border-top #D9D8D8 5px solid
	border-bottom #D9D8D8 5px solid
ul
	list-style none!important
	margin 0!important
	padding 0!important
.iframe-gmap
	height 20%
	width 100%
.iframe-detalle
	overflow-y scroll
	-webkit-overflow-scrolling touch
#frameTramite
	overflow-y scroll
	-webkit-overflow-scrolling touch
	
pre
	word-break break-word
	font-family inherit
	font-size inherit
	line-height 120%
p
	color #6E6E6E
	font-weight 300
	font-size 5vw
	margin 10px 0
	@media screen and (min-width: 640px)
		font-size 3vw
object
	width 100%
	height 100%
